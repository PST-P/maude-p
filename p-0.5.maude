  eq [if-then-else] :
     [[MId, Id, St, Events, Events', if (E) { Code } else { Code' } Code'' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| if (@{E}) { Code } else { Code' } Code'' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [if-then-else] :
     [[MId, Id, St, Events, Events', result(BE) <| if (@{E}) { Code } else { Code' } Code'' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', if BE then Code else Code' fi Code'' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  eq [return] :
     [[MId, Id, St, Events, Events', return ; <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [return] :
     [[MId, Id, St, Events, Events', return E ; <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| return @ ; <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log] .
  eq [return] :
     [[MId, Id, St, Events, Events', result(E) <| return @ ; <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', result(E) <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log] .

  eq [function-end] :
     [[MId, Id, St, Events, Events', skip <<| @ ; Code <| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [function-end] :
     [[MId, Id, St, Events, Events', @ ; <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [function-end] :
     [[MId, Id, St, Events, Events', @ ; Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log] .
  eq [block-end] :
     [[MId, Id, St, Events, Events', skip <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', CS, FS, DEs, IEs, MS] ES, S, Sys, Log] [owise] .

  rl [goto] :
     [[MId, Id, St, Events, Events', goto Id' ; Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, (Machines, Functions, Tests), Log]
  => [[MId, Id, Id', mtq, Events << Events', p2 getStates(get(Machines, Id))[Id'], FS, p4 getStates(get(Machines, Id))[Id'], p5 getStates(get(Machines, Id))[Id'], mtMemory <m| MS] ES, S, (Machines, Functions, Tests), Log] .

  eq [goto] :
     [[MId, Id, St, Events, Events', goto Id', E ; Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events, Events', eval(E) <| goto Id', @ ; Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, (Machines, Functions, Tests), Log] .
  crl [goto] :
     [[MId, Id, St, Events, Events', result(E) <| goto Id', @ ; Code <<| CS, FS, DEs, IEs, MM <m| MS] ES, S, (Machines, Functions, Tests), Log]
   => [[MId, Id, Id', mtq, Events << Events', p2 getStates(get(Machines, Id))[Id'], FS, p4 getStates(get(Machines, Id))[Id'], p5 getStates(get(Machines, Id))[Id'], [VId -> E] <m| MS] ES, S, (Machines, Functions, Tests), Log] 
   if (VId : Ty) := p1 (getStates(get(Machines, Id))[Id']) .

  ---- announce instructions are ignored
  eq [event-announce] :
     [[MId, Id, St, Events, Events', announce EN, E ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-announce] :
     [[MId, Id, St, Events, Events', announce EN ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  rl [event-raise] :
     [[MId, Id, St, Events, Events', raise EN ; Code <<| CS, FS, DEs, IEs, MS <m| Mem] ES, S, Sys, Log]
  => [[MId, Id, St, Events, EN{noValue} << Events', skip, FS, DEs, IEs, Mem] ES, S, Sys, Log] .

  eq [event-raise] :
     [[MId, Id, St, Events, Events', raise EN, E ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| raise EN, @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  rl [event-raise] :
     [[MId, Id, St, Events, Events', result(E) <| raise EN, @ ; Code <<| CS, FS, DEs, IEs, MS <m| Mem] ES, S, Sys, Log]
  => [[MId, Id, St, Events, EN{E} << Events', skip, FS, DEs, IEs, Mem] ES, S, Sys, Log] .

  eq [event-send] :
     [[MId, Id, St, Events, Events', send E, EN ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| send @, EN ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', send E, VId ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| send @, VId ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  rl [event-send1] :
     [[MId, Id, St, Events, Events', result(MId') <| send @, EN ; Code <<| CS, FS, DEs, IEs, MS] 
      [MId', Id', St', Events''', Events'', CS', FS', DEs', IEs', MS'] ES, S, Sys, Log]
  => [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] 
      [MId', Id', St', Events''', Events'' << EN{noValue}, CS', FS', DEs', IEs', MS'] ES, S, Sys, Log]
----  if Events' = mtq ---- TODO: with this condition, an event is not sent if there is one waiting to be handled
  [print "events: " Events' " <| " EN "{ noValue }"] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', send E, EN, E' ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| send @, EN, E' ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', send_`,_`,_;(E, E', E'') Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| send_`,_`,_;(@, E', E'') Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', result(MId') <| send_`,_`,_;(@, E, E') Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
    = [[MId, Id, St, Events, Events', evalEN(E) <| send_`,_`,_;(MId', @, E') Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', result(MId') <| send_`,_`,_;(@, EN, E) Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
    = [[MId, Id, St, Events, Events', eval(E) <| send_`,_`,_;(MId', EN, @) Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [event-send] :
     [[MId, Id, St, Events, Events', result(EN) <| send_`,_`,_;(MId', @, E) Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
    = [[MId, Id, St, Events, Events', eval(E) <| send_`,_`,_;(MId', EN, @) Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  rl [event-send2] :
     [[MId, Id, St, Events, Events', result(E) <| send_`,_`,_;(MId', EN, @) Code <<| CS, FS, DEs, IEs, MS] 
      [MId', Id', St', Events, Events'', CS', FS', DEs', IEs', MS'] ES, S, Sys, Log]
  => [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] 
      [MId', Id', St', Events, Events'' << EN{E}, CS', FS', DEs', IEs', MS'] ES, S, Sys, Log]
----  if Events' = mtq ---- TODO: with this condition, an event is not sent if there is one waiting to be handled
  [print "events: " Events' " <| " EN "{" E "}"] .

----  rl [halt] :
----     [[MId, Id, St, halt{noValue} << Events, skip, FS, DEs, IEs, MS] ES, S, Sys, Log]
----   => ... .

  ---- events to be ignored are removed from the in queue
  eq [event-ignore] :
     [MId, Id, St, Events, Events' << EN{noValue} << Events', CS, FS, DEs, EN # IEs, MS]
   = [MId, Id, St, Events, Events' << Events', CS, FS, DEs, EN # IEs, MS] .
  eq [event-ignore] :
     [MId, Id, St, Events, Events' << EN{E} << Events', CS, FS, DEs, EN # IEs, MS]
   = [MId, Id, St, Events, Events' << Events', CS, FS, DEs, EN # IEs, MS] .
 
  ---- Defer defers the dequeue of the event E in state S until the machine transitions to a non-deferred 
  ---- state, i.e., a state that does not defer the event E. The position of the event E that is deferred 
  ---- in state S does not change the FIFO buffer of the machine. 
  eq [event-deferred] :
     [[MId, Id, St, Events, EN{noValue} << Events', Code, FS, EN # DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events << EN{noValue}, Events', Code, FS, EN # DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] .
  eq [event-deferred] :
     [[MId, Id, St, Events, EN{E} << Events', Code, FS, EN # DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events << EN{E}, Events', Code, FS, EN # DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] .

  ---- deferred events are skipped
  ---- events are received only if there is no non-deferred event before them
  crl [event-receive] :
     [[MId, Id, St, Events, EN{noValue} << Events', skip, FS, DEs, IEs, Mem <m| MS] ES, S, (Machines, Functions, Tests), Log]
  => [[MId, Id, St, Events, Events', Code, FS, DEs, IEs, mtMemory <m| MS] ES, S, (Machines, Functions, Tests), Log]
  if (mtNamedTypeSequence, Code) := (p3 getStates(get(Machines, Id))[St])[EN] .
  crl [event-receive] :
     [[MId, Id, St, Events, EN{E} << Events', skip, FS, DEs, IEs, Mem <m| MS] ES, S, (Machines, Functions, Tests), Log]
  => [[MId, Id, St, Events, Events', Code, FS, DEs, IEs, [VId -> E] <m| MS] ES, S, (Machines, Functions, Tests), Log]
  if (VId : Ty, Code) := (p3 getStates(get(Machines, Id))[St])[EN] .
  crl [event-receive] :
     [[MId, Id, St, Events, EN{noValue} << Events', noCode, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
  => [[MId, Id, St, Events, Events', Code, FS, DEs, IEs, mtMemory <m| MS] ES, S, (Machines, Functions, Tests), Log]
  if (mtNamedTypeSequence, Code) := (p3 getStates(get(Machines, Id))[St])[EN] .
  crl [event-receive] :
     [[MId, Id, St, Events, EN{E} << Events', noCode, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
  => [[MId, Id, St, Events, Events', Code, FS, DEs, IEs, [VId -> E] <m| MS] ES, S, (Machines, Functions, Tests), Log]
  if (VId : Ty, Code) := (p3 getStates(get(Machines, Id))[St])[EN] .

  rl [event-receive1] :
     ---- TODO: if there are several EN events, should the first one be taken? Now it's non-deterministically taking one 
     [[MId, Id, St, Events, Events' << EN{noValue} << Events'', receive { case EN : { Code } RCs } Code' <<| CS, FS, DEs, IEs, MS] ES, 
      S, (Machines, Functions, Tests), Log]
   => [[MId, Id, St, Events, Events' << Events'', Code Code' <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] .
  rl [event-receive2] :
     [[MId, Id, St, Events, Events' << EN{E} << Events'', receive { case EN : (VId : Ty) { Code } RCs } Code' <<| CS, FS, DEs, IEs, Mem <m| MS] ES, 
      S, (Machines, Functions, Tests), Log]
   => [[MId, Id, St, Events, Events' << Events'', Code Code' <<| CS, FS, DEs, IEs, [VId -> E] Mem <m| MS] ES, S, (Machines, Functions, Tests), Log] .
  rl [event-receive1] :
     [[MId, Id, St, Events, Events' << EN{noValue} << Events'', receive { case EN : { } RCs } Code' <<| CS, FS, DEs, IEs, MS] ES, 
      S, (Machines, Functions, Tests), Log]
   => [[MId, Id, St, Events, Events' << Events'', Code' <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] .
  rl [event-receive2] :
     [[MId, Id, St, Events, Events' << EN{E} << Events'', receive { case EN : (VId : Ty) { } RCs } Code' <<| CS, FS, DEs, IEs, MS] ES, 
      S, (Machines, Functions, Tests), Log]
   => [[MId, Id, St, Events, Events' << Events'', Code' <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] .

  eq [void-function-invocation9] : 
     [[MId, Id, St, Events, Events', F() ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(F()) <| @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [void-function-invocation10] : 
     [[MId, Id, St, Events, Events', F(Exprs) ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(F(Exprs)) <| @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  ----  op _(_,_) ; : Name EventName Expr -> Sentence .
  eq [void-function-invocation10-1] : 
     [[MId, Id, St, Events, Events', F(EN, E) ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(F(EN, E)) <| @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  ceq [eval-function-invocation11] : 
     [[MId, Id, St, Events, Events', eval(F()) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', invocation(mtExprList, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] 
      ES, S, Sys, Log] 
   if fun(F, NTS, MTy, Code') := getFunction(F, FS) .
  ceq [eval-function-invocation12] : 
     [[MId, Id, St, Events, Events', eval(F()) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events, Events', invocation(mtExprList, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] 
   if noFunction = getFunction(F, FS) 
   /\ fun(F, NTS, MTy, Code') := getFunction(F, Functions) .

  ceq[eval-function-invocation13] : 
     [[MId, Id, St, Events, Events', eval(F(Exprs)) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', invocation(Exprs, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] 
   if fun(F, NTS, MTy, Code') := getFunction(F, FS) .
  ceq [eval-function-invocation14] : 
     [[MId, Id, St, Events, Events', eval(F(Exprs)) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events, Events', invocation(Exprs, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] 
   if noFunction = getFunction(F, FS) 
   /\ fun(F, NTS, MTy, Code') := getFunction(F, Functions) .

  ----  op _(_,_) ; : Name EventName Expr -> Sentence .
  ceq[eval-function-invocation13] : 
     [[MId, Id, St, Events, Events', eval(F(EN, E)) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', invocation(EN, E, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] 
   if fun(F, NTS, MTy, Code') := getFunction(F, FS) .
  ceq [eval-function-invocation14] : 
     [[MId, Id, St, Events, Events', eval(F(EN, E)) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log]
   = [[MId, Id, St, Events, Events', invocation(EN, E, NTS, Code', mtMemory) <| Code <<| CS, FS, DEs, IEs, MS] ES, S, (Machines, Functions, Tests), Log] 
   if noFunction = getFunction(F, FS) 
   /\ fun(F, NTS, MTy, Code') := getFunction(F, Functions) .

  ---- TODO: non-overloaded functions
  op getFunction : Name Set{Function} -> Maybe{Function} .
  eq getFunction(F, (fun(F, NTS, MTy, Code) # FS)) = fun(F, NTS, MTy, Code) .
  eq getFunction(F, FS) = noFunction [owise] .

  eq [function-invocation1] : 
     [[MId, Id, St, Events, Events', invocation((Expr, Exprs), (VId : Ty, NTS), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(Expr) <| invocation((Expr, Exprs), (VId : Ty, NTS), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [function-invocation3] : 
     [[MId, Id, St, Events, Events', result(E) <| invocation((Expr, Exprs), (VId : Ty, NTS), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', invocation(Exprs, NTS, Code, Mem [VId -> E]) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [function-invocation5] : 
     [[MId, Id, St, Events, Events', invocation(mtExprList, mtNamedTypeSequence, Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| Code' <<| CS, FS, DEs, IEs, Mem <m| MS] ES, S, Sys, Log] .
  eq [function-invocation6] :  
     [[MId, Id, St, Events, Events', result(E) <<| Code' <<| CS, FS, DEs, IEs, Mem <m| MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', result(E) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  ----  op _(_,_) ; : Name EventName Expr -> Sentence .
  eq [function-invocation1] : 
     [[MId, Id, St, Events, Events', invocation(EN, Expr, (VId : event, VId' : Ty), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(Expr) <| invocation(EN, @, (VId : event, VId' : Ty), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [function-invocation3] : 
     [[MId, Id, St, Events, Events', result(E) <| invocation(EN, @, (VId : event, VId' : Ty), Code, Mem) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', invocation(mtExprList, mtNamedTypeSequence, Code, Mem [VId -> EN] [VId' -> E]) <| Code' <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

----  eq [assert] : 
----     [[MId, Id, St, Events, Events', assert E ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
----   = [[MId, Id, St, Events, Events', eval(E) <| assert @{E} ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
----  eq [assert] : 
----     [[MId, Id, St, Events, Events', result(true) <| assert @{E} ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
----   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
----  eq [assert] : 
----     [[MId, Id, St, Events, Events', result(false) <| assert @{E} ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
----   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .

  eq [assert] : 
     [[MId, Id, St, Events, Events', assert E, StrE ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(E) <| assert @{E}, StrE ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [assert] : 
     [[MId, Id, St, Events, Events', result(true) <| assert @{E}, StrE ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [assert] : 
     [[MId, Id, St, Events, Events', result(false) <| assert @{E}, StrE ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(StrE) <| assert @{false}, @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [assert] : 
     [[MId, Id, St, Events, Events', result(Str) <| assert @{false}, @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, (Log ++ Str) ++ "\n"] .

  eq [print] : 
     [[MId, Id, St, Events, Events', print StrE ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', eval(StrE) <| print @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log] .
  eq [print] : 
     [[MId, Id, St, Events, Events', result(Str) <| print @ ; Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, Log]
   = [[MId, Id, St, Events, Events', Code <<| CS, FS, DEs, IEs, MS] ES, S, Sys, (Log ++ Str) ++ "\n"] .
endm

